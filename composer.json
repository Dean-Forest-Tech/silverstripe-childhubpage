{
  "name": "dft/silverstripe-childhubpage",
  "description": "Simple Silverstripe module that shows child pages as either a list or grid",
  "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/Dean-Forest-Tech/silverstripe-childhubpage",
    "keywords": [
        "silverstripe",
        "children",
        "sub-pages",
        "hub",
        "list",
        "grid"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@i-lateral.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Dean-Forest-Tech/silverstripe-childhubpage/issues"
    },
    "require": {
        "silverstripe/cms": "^4 || ^5"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "DFT\\SilverStripe\\ChildHubPage\\": "src/"
        }
    }
}
